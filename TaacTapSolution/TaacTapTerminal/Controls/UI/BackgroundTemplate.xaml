<Page
    x:Class="TaacTapTerminal.Controls.BackgroundTemplate"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TaacTapTerminal.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Height="800" Width="1280">
    <Canvas Background="DarkGray" HorizontalAlignment="Left" Height="800" VerticalAlignment="Top" Width="1280">
        <Border Background="Black" BorderBrush="Black" BorderThickness="1" Height="100" Width="1280"/>
        <Image  Source="{Binding CompanyLogoViewModel}" x:Name="image" Height="100" Canvas.Left="1163" Width="119" RenderTransformOrigin="0.468,0.553"/>
    </Canvas>
</Page>
